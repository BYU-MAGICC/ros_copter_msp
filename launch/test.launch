<launch>
 
  <arg name="mav_name" default="hummingbird"/>

 <rosparam subst_value="True">
  xbox:           { deadzone: 0.15,
                    autorepeat_rate: 10}
  naze:           { imu_pub_rate: 250.0,
                    rc_send_rate: 50.0,
                    timeout: 1,
                    enable_GPS: false,
                    baudrate: 57600 }

 </rosparam>  

 <!-- Joystick -->
 <rosparam command="load" file="$(find joy_commander)/params/ps2.yaml"/>
  <node name="xbox"                pkg="joy"                 type="joy_node"/>
 <node name="joy_sim" pkg="joy_sim" type="joy_sim" >
    <rosparam command="load" file="$(find relative_nav)/param/$(arg mav_name).yaml"/>
    <rosparam command="load" file="$(find relative_nav)/param/xbox.yaml"/>
  </node>

 <rosparam command="load" file="$(find naze_ros)/param/rc.yaml"/>
 <node name="naze"                pkg="naze_ros"            type="naze_ros_node" output="screen">
 	<rosparam command="load" file="$(find relative_nav)/param/$(arg mav_name).yaml"/>
 	<param name="serial_port" value="/dev/ttyUSB0"/>
 </node>

</launch>


